align=center
separator_block_width=5

[date]
command=echo -ne " " && date +"%a %e %b"
interval=1
min_width=125
separator=false

[time]
command=echo -ne " " && date +%H:%M
interval=1
min_width=100
separator=false

[dunst]
command=~/.i3scr/dunst-state.sh
interval=1
min_width=100
color=#ff79c6

[volume]
command=~/.i3scr/volume.sh get
interval=1
min_width=125

[kbd-layout]
command=~/.i3scr/kbd-layout.sh
interval=1
min_width=100
